;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	devops.example.lan. root.devops.example.lan. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	example.lan.
@	IN	A	192.168.1.12
devops	IN	A	192.168.1.12
